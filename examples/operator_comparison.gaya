(* Output: '1\n0\n1\n0\n0\n1\n1\n1\n0\n1' *)

x :: 1
y :: 2

discard io.println(x <  y)
discard io.println(x >  y)
discard io.println(x <= y)
discard io.println(x >= y)
discard io.println(x == y)

x :: y

discard io.println(x == y)
discard io.println(x <= y)
discard io.println(x >= y)

(* Different types are not equal. *)
discard io.println("str" == 42)
discard io.println("str" /= 42)
