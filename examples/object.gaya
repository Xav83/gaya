(* Output: 'Alina\nAlina\nDoes not understand' *)

Person :: { name =>
  { msg =>
    cases
      given msg == "getName" => name
      given msg == "print"   => io.println(name)
      otherwise              => io.println("Does not understand")
    end
  }
}

p :: Person("Alina")

discard "getName" |> p(_) |> io.println(_)

discard "print" |> p(_)

discard "someOtherMessage" |> p(_)

